{
    "name": "directus-extension-board-layout",
    "icon": "extension",
    "version": "1.1.2",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-layout"
    ],
    "directus:extension": {
        "type": "layout",
        "path": "../../directus-bit-store/extensions/layouts/7Span-kanban/index.js",
        "source": "src/index.ts",
        "host": "^9.22.4"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:shocota/directus-extension-board-layout.git"
    },
    "scripts": {
        "build": "directus-extension build -w",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link",
        "semantic-release": "semantic-release",
        "type-check": "npx tsc --noEmit",
        "lint": "npx eslint src/**/*"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@directus/extensions-sdk": "9.23",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@semantic-release/git": "^10.0.1",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.9.0",
        "husky": "^8.0.3",
        "prettier": "^2.8.3",
        "sass": "^1.63.6",
        "semantic-release": "^20.0.2",
        "typescript": "^4.9.4",
        "vue": "^3.2.45",
        "vuedraggable": "^4.1.0"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            [
                "@semantic-release/github",
                {
                    "assets": [
                        {
                            "path": "dist/index.js",
                            "label": "index.js"
                        }
                    ]
                }
            ],
            "@semantic-release/git"
        ],
        "branches": [
            "main"
        ]
    },
    "dependencies": {
        "@directus/composables": "^10.0.6",
        "@directus/format-title": "^10.0.0",
        "@directus/types": "^10.1.2",
        "@directus/utils": "^10.0.6",
        "@vue/eslint-config-typescript": "^11.0.2",
        "axios": "^1.4.0",
        "mitt": "^3.0.1",
        "p-queue": "^7.3.4",
        "vue-i18n": "^9.2.2"
    }
}
